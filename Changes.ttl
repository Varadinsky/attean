                        doap:created      "2015-08-18" ;
                        ];
        asc:changes     [
                        asc:addition    "Added heap sort plan implementation." ;
                        asc:addition    "Added Attean::API::Plan::Join role." ;
                        asc:addition    "Added apply_triple and apply_quad methods to triple and quad pattern classes to produce Result objects." ;
                        asc:update      "Moved computation of in_scope_variables from calling code to to Plan class BUILDARGS." ;
                        asc:update      "Consolidated BUILDARGS handling in Attean::API::TripleOrQuadPattern." ;
