<http://kasei.us/code/files/Attean-0.009.tar.gz>
        dcterms:isVersionOf <http://kasei.us/code/attean/#project> ;
        dcterms:replaces    <http://kasei.us/code/files/Attean-0.008.tar.gz> ;

        doap:Version    [
                        doap:revision     "0.009" ;
                        doap:created      "2015-XX-XX" ;
                        ];
        asc:changes     [
                        asc:addition    "Added Attean::API::Result->shared_domain method." ;
                        asc:update      "Improve query planner and plan implementations to support SPARQL 1.1 test suite." ;
                        asc:update      "Removed HeapSort plan implementation and use of Array::Heap due to packaging concerns (issue #32)." ;
                        asc:update      "Improve handling on unicode data in SPARQL TSV parser." ;
                        ] .


<http://kasei.us/code/files/Attean-0.008.tar.gz>
        dcterms:isVersionOf <http://kasei.us/code/attean/#project> ;
        dcterms:replaces    <http://kasei.us/code/files/Attean-0.007.tar.gz> ;

        doap:Version    [
